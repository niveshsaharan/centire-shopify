{
    "name": "centire/shopify",
    "version": "7.0.3",
    "description": "Shopify App Package",
    "keywords": [
        "shopify",
        "centire"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Nivesh",
            "email": "niveshsaharan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "laravel/framework": "~7.0",
        "doctrine/dbal": "^2.9"
    },
    "require-dev":{
        "orchestra/database": "^5.0",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Centire\\ShopifyApp\\": "src/ShopifyApp"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Centire\\ShopifyApp\\Test\\": "tests"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Centire\\ShopifyApp\\ShopifyAppProvider"
            ]
        }
    }
}
