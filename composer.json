{
    "name": "centire/shopify",
    "version": "2.1.6",
    "description": "Shopify App Package",
    "keywords": [
        "shopify",
        "centire"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Nivesh",
            "email": "niveshsaharan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "~5.5",
        "doctrine/dbal": "~2.9"
    },
    "require-dev":{
    "orchestra/database": "~3.5",
    "orchestra/testbench": "~3.5",
    "phpunit/phpunit": "~6.0||~7.0",
    "squizlabs/php_codesniffer": "^3.0"
},
    "autoload": {
        "psr-4": {
            "Centire\\ShopifyApp\\": "src/ShopifyApp"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Centire\\ShopifyApp\\Test\\": "tests"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Centire\\ShopifyApp\\ShopifyAppProvider"
            ]
        }
    }
}
